# Theme configuration
theme my_custom_theme:
prompt_fg=214     # Orange prompt
prompt_bg=32
text_fg=255       # Bright white text
error_fg=196      # Bright red errors
dir_fg=81         # Light blue directories
highlight_fg=226  # Bright yellow highlights
prompt_symbol=Î»   # Custom prompt symbol
env_prompt_symbol=$ # Custom env prompt symbol

